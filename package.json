{
  "name": "vpv",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html",
    "lint": "standardx '**/*.ts' '**/*.tsx'",
    "graphql-generate": "graphql-codegen --config .graphql-codegen.yml"
