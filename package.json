    "graphql-anywhere": "^4.2.6",
    "graphql-tag": "^2.10.1",
    "leaflet": "^1.5.1",
    "luxon": "^1.21.3",
    "parcel": "^1.12.3",
    "query-string": "^6.9.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-leaflet": "^2.4.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "src": "^1.1.2",
