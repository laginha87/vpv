    "@types/react-router-dom": "^5.1.0",
    "@types/webpack-env": "^1.14.1",
    "@types/yup": "^0.26.26",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "apollo": "^2.21.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "parcel-bundler": "^1.12.4",
    "postcss-import": "^12.0.1",
    "standardx": "^5.0.0",
    "tslint": "^5.20.0",
    "typescript": "^3.7.1"
