{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~*": [
                "*"
            ]
        },
        "target": "es5",
        "strict": true,
        "jsx": "react",
        "noEmit": true,
        "lib": [
            "dom"
        ],
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false,
    },
    "include": ["tailwind.config.js"],
    "exclude": ["node_modules/"]
}